    super.key,
    required this.eventName,
    bool? ageFieldVisible,
    required this.riderAge,
  }) : this.ageFieldVisible = ageFieldVisible ?? false;

  final String? eventName;
  final bool ageFieldVisible;
  final String? riderAge;

  static String routeName = 'RegEndurance';
